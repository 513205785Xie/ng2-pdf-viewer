    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "systemjs": "^0.20.19",
    "systemjs-builder": "^0.16.12",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "zone.js": "^0.8.18"
  },
  "files": [
    "dist"
  ]
